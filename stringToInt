
public class changeStringToInt 
	{
		public static void main (String[] args)
			{
				String myString = "127";
				int a = changeStringToInt.changeString(myString);
				System.out.println(a);
			}

			
			//method definition
		static int changeString(String myString)
			{
				int answer = 0; 
				for (int i = myString.length()-1; i >= 0;)
				{
					answer += (myString.charAt(i) - '0');
					return answer;  		
			    }
				return answer;
			}
	}
